{
  "expo": {
    "name": "cencrypt",
    "slug": "cencrypt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#D9D9D9"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/favicon.png",
        "backgroundColor": "#D9D9D9"
      },
      "package": "com.centxz.cencrypt"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/logo/logo.png"
    }
  }
}
